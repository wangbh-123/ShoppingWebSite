<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wbh.dao.statusDao">
    <insert id="insert1"/>

    <update id="update1"/>

    <delete id="delete1"/>

    <select id="findAll1" resultType="com.wbh.po.status">
    select * from status </select>

    <select id="findByid1" resultType="com.wbh.po.status">
    select * from status where status_id=#{status_id} </select>
    <resultMap id="statusMap" type="com.wbh.po.status">

    <id property="status_id" column="status_id"/>

    <result property="status_name" column="status_name"/>

</resultMap>
</mapper>